{
  "riveMeta": {
    {{#riv_files}}
    "{{riv_camel_case}}": {
      "artboards": {
        {{#artboards}}
        "{{artboard_camel_case}}": {
          "name": "{{artboard_name}}",
          "animations": {
            {{#animations}}
            "{{animation_camel_case}}": "{{animation_name}}"{{^last}},{{/last}}
            {{/animations}}
          },
          "stateMachines": {
            {{#state_machines}}
            "{{state_machine_camel_case}}": {
              "name": "{{state_machine_name}}",
              "inputs": {
                {{#inputs}}
                "{{input_camel_case}}": {
                  "name": "{{input_name}}",
                  "type": "{{input_type}}",
                  "defaultValue": "{{input_default_value}}"
                }{{^last}},{{/last}}
                {{/inputs}}
              }
            }{{^last}},{{/last}}
            {{/state_machines}}
          }
        }{{^last}},{{/last}}
        {{/artboards}}
      }
    }{{^last}},{{/last}}
    {{/riv_files}}
  }
}